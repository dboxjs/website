.height-header 
    height: 470px

.back-header 
    background-image: url(http://dboxjs.org/assets/images/background.png)
    background-size: cover

.displayed 
    display: block
    margin-left: auto
    margin-right: auto
    padding-top: 80px

.tex-center 
    font-size: 20px
    color: #fff
    line-height: 1.8
    font-family: 'Lato'
    margin: 40px auto
    max-width: 940px
    font-weight: 300

p 
    display: block
    -webkit-margin-before: 1em
    -webkit-margin-after: 1em
    -webkit-margin-start: 0px
    -webkit-margin-end: 0px

.btn-center, .tex-center 
    text-align: center

.btn
    border: 1px solid transparent

.btn-primary 
    color: #fff
    border-color: #fff
    background-color: hsla(0,0%,100%,0)
    font-family: 'Lato'
    font-size: 12px
    height: 45px
    width: 200px
    letter-spacing: 2px
    margin: 0 12px
    border-radius: 6px
